                         {
                         	let _m = this._m;
                         	if(this.estado==0)
                         	{
                             this._r += ((_m[1]+5)-this._r)/20;
                              if(this._r>_m[1])
                              {
                              this.estado=1;                         
                         	  }
                              _room.setPlayerDiscProperties(u.id, {radius: Math.floor(this._r)   } );
 
                         	}
                         	if(this.estado==1)
                         	{
                              if(this.tt[0]>this.tt[1]-(60*2) )
                              {
                              	this.estado=2;
                              }
                         	}
 
                            if(this.estado==2)
                         	{
                              this._r += ((_m[0]-5)-this._r)/20;
                              if(this._r<_m[0])
                              {
                              this.estado=4;
                              this._r = _m[0];                         
