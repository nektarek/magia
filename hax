function get_teams_n()
{
 
       //centrinactivo
  let _team_n = [0,      0,0]
     for(var u of _room.getPlayerList())
     {
       _team_n[u.team]++;
     }
   return(_team_n);
 
}
 
 
 
_room.onTeamVictory=function(_score)
{
let _s = "rojo";
if(_score.blue>_score.red)
    _s = "azul"
 
_message.send("Partida finalizada! Ha ganado el equipo " + _s + "!");
 
 
 
 
}
 
